@model List<AHY.Identity.Entities.AppRole>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<a asp-action="Create" asp-controller="Role" class="btn btn-info">Yeni Rol Ekle</a>
<table class="table table-hover table-bordered table-striped table-sm">
    <thead>
        <tr>
            <th>Role Name</th>
            <th>Create Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.CreateTime.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
